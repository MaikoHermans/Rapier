<includes>
	<include name="QuickJumpVerticalLayout">
		<left>$PARAM[left]</left>
		<top>$PARAM[top]</top>
		<height>$PARAM[height]</height>
		<width>14</width>
		<orientation>vertical</orientation>
		<onleft>500</onleft>
		<visible>IntegerGreaterThan(Container.NumPages,1) + Skin.HasSetting(EnableQuickJump)</visible>
		<itemlayout height="$PARAM[itemheight]" width="14">
			<control type="label">
				<width>100%</width>
				<align>center</align>
				<label>$INFO[ListItem.Label]</label>
				<textcolor>MediaInfoItemColor2</textcolor>
				<shadowcolor>ShadowColor</shadowcolor>
				<font>ListFont7</font>
				<animation effect="fade" start="100" end="20" time="0" condition="!IsEmpty(ListItem.Property(NotAvailable))">Conditional</animation>
				<visible>!StringCompare(ListItem.Label,Container.ListItem.SortLetter)</visible>
			</control>
